{
  "$schema": "https://schemas.wp.org/trunk/block.json",
  "apiVersion": 2,
  "name": "edgepress/tabs",
  "version": "0.1.0",
  "title": "Tabs",
  "category": "text",
  "icon": "category",
  "description": "A Gutenberg block that allows you to format content inside of tab groups. It also allows you to setup where the tabs are displayed.",
  "attributes": {
    "tabs": {
      "type": "array",
      "source": "query",
      "query": {
        "title": {
          "type": "string",
          "source": "text",
          "selector": ".tab"
        },
        "content": {
          "type": "null"
        }
      },
      "default": [
        {
          "title": "New Tab",
          "content": null
        }
      ]
    }
  },
  "supports": {
    "html": true
  },
  "textdomain": "edgepress",
  "editorScript": "file:./index.js"
}